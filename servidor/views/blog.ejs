<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <link href="blog.css" rel="stylesheet">
  
</head>
<body>
  <div class="container">
    <h1>Blog</h1>
    <a href="/cadastrar_post" class="btn-new-post">Criar Novo Post</a>
    
    <% posts.forEach(post => { %>
      <div class="post">
        <h2><%= post.titulo %></h2>
        <p><em><%= post.resumo %></em></p>
        <p><%= post.conteudo %></p>

      </div>
    <% }); %>
  </div>
</body>
</html>